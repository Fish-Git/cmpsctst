#!/bin/bash

  OPATH=$PATH
  PATH=.:./Linux:$OPATH

  clear && cmpsctst.rexx  ./FILES/SMALL/EBCDIC  ./DICTS  ./WORK        -o1 -r 0 -z  -bb 0:0  >  ./WORK/cmpsctst-sen-b00.log
  clear && cmpsctst.rexx  ./FILES/LARGE/EBCDIC  ./DICTS  ./WORK        -o1 -r 0 -z  -bb 0:0  >  ./WORK/cmpsctst-len-b00.log
  clear && cmpsctst.rexx  ./FILES/SMALL/ASCII   ./DICTS  ./WORK     -t -o1 -r 0 -z  -bb 0:0  >  ./WORK/cmpsctst-san-b00.log
  clear && cmpsctst.rexx  ./FILES/LARGE/ASCII   ./DICTS  ./WORK     -t -o1 -r 0 -z  -bb 0:0  >  ./WORK/cmpsctst-lan-b00.log
